{
  "name": "mbtiles-server",
  "version": "1.5.0",
  "description": "Provides a compatible WMTS Tile Server from MBTiles.",
  "main": "index.js",
  "jsnext:main": "index.js",
  "scripts": {
    "start": "node cli.js --verbose",
    "test": "npm run lint && jest --coverage",
    "docs": "tsc && cat HEADER.md > README.md && documentation build index.js -c documentation.yml -f md --shallow >> README.md && cat CHANGELOG.md >> README.md",
    "lint": "tsc && tslint utils/*.ts routes/*.ts *.ts && documentation lint index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DenisCarriere/mbtiles-server.git"
  },
  "bin": {
    "mbtiles-server": "cli.js"
  },
  "author": "Denis Carriere <carriere.denis@gmail.com>",
  "contributors": [
    "Denis Carriere <carriere.denis@gmail.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DenisCarriere/mbtiles-server/issues"
  },
  "homepage": "https://github.com/DenisCarriere/mbtiles-server#readme",
  "dependencies": {
    "@mapbox/tiletype": "^0.3.0",
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "global-mercator": "^1.6.1",
    "mbtiles-offline": "^2.0.2",
    "meow": "^3.7.0",
    "wmts": "^1.4.0"
  },
  "devDependencies": {
    "coveralls": "^2.11.15",
    "documentation": "^4.0.0-beta.18",
    "jest-cli": "^18.1.0",
    "standard": "^8.6.0"
  },
  "keywords": [
    "gis",
    "geo",
    "geojs",
    "geospatial",
    "geography",
    "map",
    "mbtiles"
  ],
  "jest": {
    "verbose": true,
    "collectCoverage": true
  },
  "testEnvironment": "node",
  "standard": {
    "env": "jest"
  }
}
